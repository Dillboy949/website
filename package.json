{
    "name": "skh",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "eslint .",
        "fmt": "prettier --plugin-search-dir . --write .",
        "postinstall": "svelte-kit sync",
        "serve": "SVELTEKIT_ADAPTER=static vite build && serve -p 4000 build",
        "build:auto": "SVELTEKIT_ADAPTER=auto vite build",
        "build:node": "SVELTEKIT_ADAPTER=node vite build",
        "build:static": "SVELTEKIT_ADAPTER=static vite build",
        "build:cloudflare": "SVELTEKIT_ADAPTER=cloudflare vite build"
    },
    "devDependencies": {
        "@sveltejs/adapter-auto": "^1.0.0",
        "@sveltejs/adapter-cloudflare": "^1.0.0",
        "@sveltejs/adapter-node": "^1.1.0",
        "@sveltejs/adapter-static": "^1.0.1",
        "@sveltejs/kit": "^1.0.0",
        "@types/youtube-player": "^5.5.6",
        "@typescript-eslint/eslint-plugin": "^5.45.0",
        "@typescript-eslint/parser": "^5.45.0",
        "eslint": "^8.28.0",
        "eslint-plugin-svelte3": "^4.0.0",
        "prettier": "^2.8.0",
        "prettier-plugin-svelte": "^2.8.1",
        "sass": "^1.57.1",
        "serve": "^14.1.2",
        "svelte": "^3.54.0",
        "svelte-check": "^2.9.2",
        "tslib": "^2.4.1",
        "typescript": "^4.9.3",
        "vite": "^4.0.0"
    },
    "type": "module",
    "dependencies": {
        "@fontsource/open-sans": "^4.5.13",
        "@fontsource/roboto-slab": "^4.5.9",
        "@fortawesome/fontawesome-free": "^6.2.1",
        "youtube-player": "^5.5.2"
    }
}
