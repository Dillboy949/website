{
    "singleQuote": false,
    "trailingComma": "es5",
    "printWidth": 100,
    "plugins": ["prettier-plugin-svelte"],
    "pluginSearchDirs": ["."],
    "overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }],
    "tabWidth": 4,
    "semi": true,
    "jsxSingleQuote": false,
    "bracketSameLine": false,
    "bracketSpacing": true,
    "endOfLine": "lf"
}
